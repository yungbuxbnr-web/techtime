
{
  "name": "techtime",
  "version": "1.0.0",
  "main": "index.ts",
  "engines": {
    "node": ">=18 <23"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development expo start",
    "android": "cross-env NODE_ENV=development expo run:android",
    "ios": "cross-env NODE_ENV=development expo run:ios",
    "prebuild": "cross-env NODE_ENV=production node scripts/check-plugins.cjs && cross-env NODE_ENV=production expo prebuild --clean && node scripts/fix-gradle-wrapper.cjs",
    "export": "cross-env NODE_ENV=production expo export",
    "build:android": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' expo run:android --variant release",
    "build:ios": "cross-env NODE_ENV=production expo run:ios --configuration Release",
    "prepare": "cross-env NODE_ENV=production node -e \"process.exit(0)\"",
    "dev": "cross-env NODE_ENV=development EXPO_NO_TELEMETRY=1 expo start --tunnel",
    "dev:android": "cross-env NODE_ENV=development EXPO_NO_TELEMETRY=1 expo start --android",
    "dev:ios": "cross-env NODE_ENV=development EXPO_NO_TELEMETRY=1 expo start --ios",
    "dev:web": "cross-env NODE_ENV=development EXPO_NO_TELEMETRY=1 expo start --web",
    "build:web": "cross-env NODE_ENV=production expo export -p web && npx workbox generateSW workbox-config.cjs",
    "build:eas:android": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' eas build --platform android --profile production",
    "build:eas:ios": "cross-env NODE_ENV=production eas build --platform ios --profile production",
    "build:eas:all": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' eas build --platform all --profile production",
    "build:preview:android": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' eas build --platform android --profile preview",
    "build:preview:ios": "cross-env NODE_ENV=production eas build --platform ios --profile preview",
    "prebuild:android": "cross-env NODE_ENV=production node scripts/check-plugins.cjs && cross-env NODE_ENV=production expo prebuild -p android --clean && node scripts/fix-gradle-wrapper.cjs",
    "prebuild:ios": "cross-env NODE_ENV=production node scripts/check-plugins.cjs && cross-env NODE_ENV=production expo prebuild -p ios --clean",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "fix:gradle": "node scripts/fix-gradle-wrapper.cjs",
    "gradle:stop": "cd android && ./gradlew --stop",
    "gradle:clean": "cd android && ./gradlew clean --no-daemon",
    "gradle:refresh": "npm run gradle:stop && npm run gradle:clean",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/runtime": "^7.26.9",
    "@expo/config-plugins": "~54.0.0",
    "@expo/metro-runtime": "~6.1.1",
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.3.0",
    "@react-native-picker/picker": "^2.11.2",
    "@react-navigation/drawer": "^7.1.1",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@types/difflib": "^0.2.7",
    "difflib": "^0.2.4",
    "expo": "~54.0.1",
    "expo-auth-session": "~7.0.0",
    "expo-background-fetch": "~14.0.0",
    "expo-blur": "~15.0.0",
    "expo-camera": "~17.0.0",
    "expo-constants": "~18.0.0",
    "expo-crypto": "~15.0.0",
    "expo-document-picker": "~14.0.0",
    "expo-file-system": "~19.0.0",
    "expo-font": "~14.0.0",
    "expo-haptics": "~15.0.0",
    "expo-image-manipulator": "~14.0.0",
    "expo-image-picker": "~17.0.0",
    "expo-linear-gradient": "~15.0.0",
    "expo-linking": "~8.0.0",
    "expo-local-authentication": "~17.0.0",
    "expo-media-library": "~18.0.0",
    "expo-notifications": "~0.32.0",
    "expo-print": "~15.0.0",
    "expo-router": "~6.0.0",
    "expo-sharing": "~14.0.0",
    "expo-splash-screen": "~31.0.0",
    "expo-status-bar": "~3.0.0",
    "expo-symbols": "~1.0.0",
    "expo-system-ui": "~6.0.0",
    "expo-task-manager": "~14.0.0",
    "expo-updates": "~29.0.0",
    "expo-web-browser": "~15.0.0",
    "pdfjs-dist": "^5.4.449",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-device-info": "^14.1.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.21.1",
    "react-native-webview": "^13.15.0",
    "workbox-cli": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/js": "^9.19.0",
    "@types/react": "~19.1.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-expo": "~54.0.7",
    "cross-env": "^7.0.3",
    "eas-cli": "^14.7.1",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.14.0",
    "typescript": "^5.8.3",
    "webpack-cli": "^6.0.1"
  },
  "resolutions": {
    "@expo/prebuild-config": "~54.0.0"
  },
  "overrides": {
    "@expo/config-plugins": "~54.0.0"
  },
  "private": true
}
